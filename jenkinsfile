pipeline {
  agent {
        label 'traxx-testops'
    }
   tools {
        nodejs 'nodejs_18'
    }
  stages {
    
    stage('installation') {
      steps {
       
        sh 'npm install cypress --save-dev'
      }
    }
    stage('run e2e tests') {
    
      steps {
        sh 'npm run cy:run'
      }
    }
  }
 
}
