pipeline {
  agent any
  tools  {
            nodejs "MY_NodeJS"
         }
  stages {
    
    stage('installation') {
      steps {
       
        bat 'npm install'  
        //sh 'npm install cypress --save-dev'
      }
    }
    stage('run e2e tests') {
    
      steps {
        bat 'npm run cy:run'
      }
    }
  }
 
}